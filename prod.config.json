{
  "certificate": {
    "key": "certificates/jsreport.net.key",
    "cert": "certificates/jsreport.net.cert"
  },
  "connectionString": {
    "name": "neDB"
  },
  "extensions": [
    "excel-parser",
    "xlsx",
    "html-to-xlsx",
    "express",
    "templates",
    "html",
    "phantom-pdf",
    "scripts",
    "data",
    "images",
    "statistics",
    "reports",
    "childTemplates",
    "sample-template"
  ],
  "httpPort": 3003,
  "blobStorage": "fileSystem",
  "cluster": {
    "numberOfWorkers": 2,
    "enabled": false
  },
  "phantom": {
    "numberOfWorkers": 2,
    "timeout": 180000
  },
  "tasks": {
    "numberOfWorkers": 2,
    "timeout": 10000
  }
}